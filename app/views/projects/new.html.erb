<h2>New Project</h2>
<%= form_for @project do |f| %>
	<fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
  
    <%= f.label :description %>
    <%= f.text_field :description %>
  
  <fieldset>

  <%= link_to 'Cancel', projects_path, class: 'btn' %>
  <%= f.submit nil, class: 'btn btn-primary' %>
<% end -%>